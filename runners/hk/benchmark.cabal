                       directory            >= 1.3.0.0,
                       logfloat             >= 0.13 && < 0.14,
                       process              >= 1.1  && < 2.0,
                       temporary            >= 1.2.1.1
                       
Executable getNews
    Hs-Source-Dirs:    .
    Main-is:           GetNews.hs
    GHC-Options:       -O2 -threaded
    other-modules:     News
    build-depends:     base                 >= 4.7  && < 5.0,
                       unordered-containers >= 0.2.8.0,
                       containers           >= 0.5.7.1,
                       mtl                  >= 2.2.1,
                       hashable             >= 1.2.6.1,
                       directory            >= 1.3.0.0,
                       filepath             >= 1.1,
                       bytestring           >= 0.10,
                       logfloat             >= 0.13 && < 0.14,
                       mwc-random           >= 0.13 && < 0.14,
                       vector               >= 0.10
    default-language:  Haskell2010

