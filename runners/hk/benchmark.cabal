                       process              >= 1.1  && < 2.0,
                       temporary            >= 1.2.1.1,
                       time                 >= 1.6.0.1,
                       split                >= 0.2.3.2,
                       directory            >= 1.3.0.0

Executable gmmAccuracy
    Hs-Source-Dirs:    .
    Main-is:           GmmGibbs/Accuracy.hs
    Default-Language:  Haskell2010
    GHC-Options:       -O2 -fwarn-tabs
    other-modules:     Utils
    build-depends:     base                 >= 4.7  && < 5.0,
                       containers           >= 0.5.7.1,
                       vector               >= 0.10,
                       mwc-random           >= 0.13 && < 0.14,
                       hakaru               >= 0.6,
                       time                 >= 1.6.0.1,
                       split                >= 0.2.3.2,
                       filepath             >= 1.3,
