name:                benchmark
version:             0.1
synopsis:            package for testing hakaru
author:              Rajan Walia
maintainer:          walrajan@gmail.com

build-type:          Simple
cabal-version:       >=1.16                   

Executable clinicalTrial
    Hs-Source-Dirs:    .
    Main-is:           ClinicalTrial/Main.hs
    Default-Language:  Haskell2010
    GHC-Options:       -O2 -Wall -fwarn-tabs
    other-modules:     ClinicalTrial.Prog
    build-depends:     base                 >= 4.7  && < 5.0,
                       mwc-random           >= 0.13 && < 0.14,
                       hakaru               >= 0.6,
                       logfloat             >= 0.13 && < 0.14,
                       text                 >= 0.11 && < 1.3,
                       pretty               >= 1.1  && < 1.2,
                       filepath             >= 1.3,
                       vector               >= 0.10,
                       bytestring           >= 0.10,
                       process              >= 1.1  && < 2.0,
                       temporary            >= 1.2.1.1,
                       split                >= 0.2.3.2,
                       time                 >= 1.6.0.1,
                       directory            >= 1.3.0.0,
                       deepseq              >= 1.4.2.0

Executable gmmGibbs
    Hs-Source-Dirs:    .
    Main-is:           GmmGibbs/Main.hs
    Default-Language:  Haskell2010
    GHC-Options:       -O2 -Wall -fwarn-tabs
